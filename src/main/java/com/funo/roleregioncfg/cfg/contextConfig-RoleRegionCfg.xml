<?xml version="1.0" encoding="UTF-8"?>
<beans default-lazy-init="true" xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dwr="http://www.directwebremoting.org/schema/spring-dwr"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
	http://www.directwebremoting.org/schema/spring-dwr http://www.directwebremoting.org/schema/spring-dwr-2.0.xsd">

	<!-- Dao -->
	<bean id="roleRegionCfgDao" parent="abstractDao">
		<property name="proxyInterfaces" value="com.funo.roleregioncfg.dao.IRoleRegionCfgDao" />
		<property name="target">
			<bean parent="abstractSysDaoTarget">
				<constructor-arg value="com.funo.roleregioncfg.model.RoleRegionCfg" />
			</bean>
		</property>
	</bean>
		
	<!-- Adapt -->
	<bean id="roleRegionCfgAdapt" parent="adaptProxy">
		<property name="target">
			<bean class="com.funo.roleregioncfg.adapt.impl.RoleRegionCfgAdapt" parent="abstractAdapt" autowire="byName" />
		</property>
	</bean>	
	
	<!-- Mgmt -->	
	<bean id="roleRegionCfgMgmt" parent="sysTranProxy">
		<property name="target">
			<bean class="com.funo.roleregioncfg.mgmt.impl.RoleRegionCfgMgmt" parent="abstractMgmt" autowire="byName" />
		</property>
	</bean>
	
	<!-- Action -->
	<bean id="RoleRegionCfgAction" class="com.funo.roleregioncfg.action.RoleRegionCfgAction">
		<property name="roleRegionCfgMgmt" ref="roleRegionCfgMgmt"></property>
		<dwr:remote javascript="RoleRegionCfgAction" />
	</bean>
</beans>
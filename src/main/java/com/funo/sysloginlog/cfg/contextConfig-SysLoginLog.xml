<?xml version="1.0" encoding="UTF-8"?>
<beans default-lazy-init="true" xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dwr="http://www.directwebremoting.org/schema/spring-dwr"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
	http://www.directwebremoting.org/schema/spring-dwr http://www.directwebremoting.org/schema/spring-dwr-2.0.xsd">

	<!-- Dao -->
	<bean id="sysLoginLogDao" parent="abstractDao">
		<property name="proxyInterfaces" value="com.funo.sysloginlog.dao.ISysLoginLogDao" />
		<property name="target">
			<bean parent="abstractSysDaoTarget">
				<constructor-arg value="com.funo.sysloginlog.model.SysLoginLog" />
			</bean>
		</property>
	</bean>
		
	<!-- Adapt -->
	<bean id="sysLoginLogAdapt" parent="adaptProxy">
		<property name="target">
			<bean class="com.funo.sysloginlog.adapt.impl.SysLoginLogAdapt" parent="abstractAdapt" autowire="byName" />
		</property>
	</bean>	
	
	<!-- Mgmt -->	
	<bean id="sysLoginLogMgmt" parent="sysTranProxy">
		<property name="target">
			<bean class="com.funo.sysloginlog.mgmt.impl.SysLoginLogMgmt" parent="abstractMgmt" autowire="byName" />
		</property>
	</bean>
	
	<!-- Action -->
	<bean id="SysLoginLogAction" class="com.funo.sysloginlog.action.SysLoginLogAction">
		<property name="sysLoginLogMgmt" ref="sysLoginLogMgmt"></property>
		<dwr:remote javascript="SysLoginLogAction" />
	</bean>
</beans>